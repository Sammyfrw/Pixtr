<h2> Create a new gallery!</h2>

<%= form_for @gallery, url: "/galleries" do |form| %>
  <div>
    <%= form.label :name%>
    <%= form.text_field :name %>
  </div>

  <div>
      <%= form.label :description%>
      <%= form.text_area :description %>
  </div>

  <%= form.submit "Create gallery"%>
  
<% end%>

