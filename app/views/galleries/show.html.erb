<h2><%= @gallery.name %> proudly owned by <%=@gallery.user.email%>.</h2>
<p><%=@gallery.description %> </p>
<% @gallery.images.each do |image| %>
  <%= image_tag(image.url)%>
  <% if current_user == @gallery.user%>
  <%= link_to "Edit image", [:edit, @gallery, image]%>
  Created <%= time_ago_in_words image.created_at %> ago
  <% end%>

<%end%>
<p>
<% if current_user == @gallery.user%>
    <%= link_to "Edit gallery", [:edit, @gallery]%>
    <%= link_to "Delete gallery", @gallery, method: :delete%>
    <%= link_to "Add image", [:new, @gallery, :image] %>
  <% end%>
</p>